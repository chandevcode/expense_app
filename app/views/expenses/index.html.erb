
<%= link_to "New Expense", new_expense_path %>
<div class="overflow-x-auto">
  <table class="table table-zebra w-full">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Amount</th>
        <th>Date</th>
        <th>Category</th>
      </tr>
    </thead> 
    <tbody>
      <% @expenses.each do |expense| %>
        <%= render "expenses/list" %> 
      <% end %>
    </tbody>
  </table>
</div>
